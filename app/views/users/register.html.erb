<h1>Sign Up</h1>

<%= form_for @user, url: register_create_path do |f| %>
  <div>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.submit "Sign Up" %>
  </div>
<% end %>
<button class="userButton"><%= link_to 'Go to Login', login_path, class:"textDecorationNone" %></button>